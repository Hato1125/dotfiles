{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "removeComments": true,
    "sourceMap": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "paths": {
      "@App/*": [ "./App/*.ts" ],
      "@Lib/*": [ "./Lib/*.ts" ],
      "@Service/*": [ "./Lib/Service/*.ts" ],
      "@Other/*": [ "./Other/*.ts" ],
    }
  }
}